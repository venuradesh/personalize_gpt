{
  "name": "personalize-gpt",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 4200 -o",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "gen-c": "bash -c 'ng g c $1 core/components/$npm_config_component_name --prefix pgpt --change-detection OnPush'",
    "gen-c-p": "bash -c 'ng g c $1 pages/$npm_config_name --prefix pgpt'",
    "gen-s": "bash -c 'ng g s services/$npm_config_service_name'",
    "test": "node --max_old_space_size=10240 ./node_modules/jest/bin/jest --watch",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.0.0",
    "@angular/cdk": "^17.3.10",
    "@angular/common": "^17.0.0",
    "@angular/compiler": "^17.0.0",
    "@angular/core": "^17.0.0",
    "@angular/forms": "^17.0.0",
    "@angular/material": "^17.3.10",
    "@angular/platform-browser": "^17.0.0",
    "@angular/platform-browser-dynamic": "^17.0.0",
    "@angular/router": "^17.0.0",
    "ngx": "^2.4.0",
    "ngx-cookie-service": "^19.0.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "typeface-muli": "^1.1.13",
    "zone.js": "~0.14.2"
  },
  "devDependencies": {
    "@angular-builders/jest": "^17.0.3",
    "@angular-devkit/build-angular": "^17.0.3",
    "@angular/cli": "^17.0.3",
    "@angular/compiler-cli": "^17.0.0",
    "@types/jest": "^29.5.12",
    "inquirer": "^10.2.1",
    "jest": "^29.7.0",
    "jest-preset-angular": "^13.1.6",
    "ts-jest": "^29.2.4",
    "typescript": "~5.2.2"
  }
}
